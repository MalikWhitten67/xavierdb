{
    "version": 2,
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/bun"
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/" }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        } 
    ]
}
